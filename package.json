{
  "name": "rollup-plugin-filesize",
  "version": "5.0.0",
  "description": "A rollup plugin to show filesize in the cli",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src",
    "dist",
    "README"
