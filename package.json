{
  "name": "rollup-plugin-filesize",
  "version": "8.0.1",
  "description": "A rollup plugin to show filesize in the cli",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "src",
    "dist",
    "README",
    "!.DS_Store",
    "!.nyc_output"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ava test/index.test.js",
    "remove-dist": "rimraf dist/*",
    "pretest": "npm run remove-dist && rollup -c",
    "prepare": "npm run test"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritz078/rollup-plugin-filesize.git"
  },
  "keywords": [
    "rollup-plugin"
  ],
  "author": "Ritesh Kumar",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/rollup-plugin-filesize/issues"
  },
  "homepage": "https://github.com/ritz078/rollup-plugin-filesize#readme",
  "dependencies": {
    "boxen": "^4.2.0",
    "brotli-size": "4.0.0",
    "colors": "^1.4.0",
    "filesize": "^6.1.0",
    "gzip-size": "^5.1.1",
    "pacote": "^11.1.6",
    "terser": "^4.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "ava": "^3.8.1",
    "babel-eslint": "^10.1.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "esm": "^3.2.25",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3"
  },
  "ava": {
    "require": [
      "esm",
      "./test/bootstrap/register.js"
    ]
  }
}
