{
  "name": "rollup-plugin-filesize",
  "version": "9.1.2",
  "description": "A rollup plugin to show filesize in the cli",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "src",
    "dist",
    "index.d.ts",
    "README",
    "!.DS_Store",
    "!.nyc_output"
  ],
  "typings": "./index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ava --serial test/index.test.js",
    "remove-dist": "rimraf dist/*",
    "pretest": "rollup -c",
    "prepare": "npm run remove-dist && npm run test"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritz078/rollup-plugin-filesize.git"
  },
  "keywords": [
    "rollup-plugin"
  ],
  "author": "Ritesh Kumar",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/rollup-plugin-filesize/issues"
  },
  "homepage": "https://github.com/ritz078/rollup-plugin-filesize#readme",
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "boxen": "^7.0.0",
    "brotli-size": "4.0.0",
    "colors": "1.4.0",
    "filesize": "^10.0.5",
    "gzip-size": "^7.0.0",
    "pacote": "^15.0.6",
    "terser": "^5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "ava": "^5.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0"
  },
  "ava": {
    "require": [
      "esm",
      "./test/bootstrap/register.js"
    ]
  }
}
